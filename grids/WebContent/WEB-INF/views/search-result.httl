<!DOCTYPE html>
#set(List<String> hits)
<html>
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
	<title>搜索结果</title>
	<link href="$!{rs}/css/bootstrap.css" rel="stylesheet" type="text/css" media="screen" />
	<link href="$!{rs}/css/home.css" rel="stylesheet" type="text/css" media="screen" />
</head>
<body>
$!{include("navbar.httl")}
<div id="json-holder" style="display:none;">
	#for(hit : hits)
	<span class="hit-json">$!{hit}</span>
	#end
</div>
<div id="container" class="container">
	<div id="result-list"></div>
</div>
$!{include("proto.httl")}
<script src="$!{rs}/js/jquery-1.9.1.js" type="text/javascript"></script>
<script src="$!{rs}/js/bootstrap.js" type="text/javascript"></script>
<script src="$!{rs}/js/stream.js" type="text/javascript"></script>
<script src="$!{rs}/js/tag.js" type="text/javascript"></script>
<script src="$!{rs}/js/usercard.js" type="text/javascript"></script>
<script src="$!{rs}/js/search-result.js" type="text/javascript"></script>
</body>
</html>